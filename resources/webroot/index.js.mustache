// eslint-disable-next-line no-unused-vars
function editGymName(id, oldName) {
  const newName = prompt('Edit Name for Gym: ' + id, oldName !== 'null' ? oldName : '')
  if (!newName) return
  $.ajax({
    url: '/api/set_data',
    data: {
      set_gym_name: true,
      gym_id: id,
      gym_name: newName,
      _csrf: '{{csrf}}'
    },
    type: 'post',
    success: function() {
      const gym = gymMarkers.find(function (value) { return value.id === id })
      gym.name = newName
      updateOpenedPopupLoop()
    },
    error: function(xhr) {
      console.error('/api/get_data returned status code ' + xhr.status + ' (' + xhr.statusText + ')')
    }
  })
}

